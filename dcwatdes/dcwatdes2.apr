et(\"strEpanet\")\nelse\n  'fallback for development purposes\n  strEpanet = \"c:\\program files\\epanet2\\epanet2d.exe\"\nend\n\n' this will not export spatial data\nfln_Xml = av.run(\"DCWatDes.Model.Epanet.writeInpFile\", false)\n\nstrXml= fln_Xml.getFullName\nstrBasePath=strXml.left(strXml.count-3)\n\nflnInp = (strBasePath+\"inp\").asFilename\nflnOut = (strBasePath+\"out\").asFilename\n\n'clean up\nFile.delete(flnOut)\n\n' run Epanet\n\nSystem.execute(strEpanet.quote++(strBasePath+\"inp\").quote++(strBasePath+\"out\").quote) \n\n' show Epanet
 Report\n\n' wait for the report file appearing\n\nkernel32 = DLL.Make(\"c:\\WINNT\\system32\\kernel32.dll\".AsFileName)\nsleep = DLLProc.Make(kernel32, \"Sleep\", #DLLPROC_TYPE_VOID, {#DLLPROC_TYPE_INT32})\n\ntimeStart=Date.now\nwhile(File.exists(flnOut).not)\n  if((Date.now-timeStart).asSeconds>20)then\n    MsgBox.info(\"No Report file found. Check your settings.\",\"DC Water Design Extension\")\n    exit\n  end\n  if(sleep <>nil)then\n    sleep.call({1000})\n  end\nend\n\ntimeStart=Date.now\nwhile(File.isWritable(flnOut).not)\n  if((D
ate.now-timeStart).asSeconds>20)then\n    MsgBox.info(\"Couldn't open Report file. Check your settings.\",\"DC Water Design Extension\")\n    exit\n  end\n  if(sleep <>nil)then\n    sleep.call({1000})\n  end\nend\n\nFile.delete((flnOut.getFullName+\".report.xml\").asFilename)\nfor each strResult in {\"links\", \"nodes\"}\n flnResult = (flnOut.getFullName+\".\"+strResult+\".txt\").asFilename\n File.delete(flnResult)\nend\nstrCmd = \"c:\\winnt\\system32\\cmd.exe /c c:\\cygwin\\bin\\sh.exe\"++(\"$AVEXT\\dcwatdes\\epanetreport2xml.sh\".asFilename.get
Fullname)++(flnOut.getFullName)++(flnOut.getFullName)+\".report.xml\"\nSystem.execute(strCmd)\n\nflnReport = (flnOut.getFullName+\".report.xml\").asFilename\n\ntimeStart=Date.now\nwhile(File.exists(flnReport).not)\n  if((Date.now-timeStart).asSeconds>60)then\n    MsgBox.info(\"No Report file found. Check your settings.\",\"DC Water Design Extension\")\n    exit\n  end\n  if(sleep <>nil)then\n    sleep.call({1000})\n  end\nend\n\ntimeStart=Date.now\nwhile(File.isWritable(flnReport).not)\n  if((Date.now-timeStart).asSeconds>60)then\n  
  MsgBox.info(\"Couldn't open Report file. Check your settings.\",\"DC Water Design Extension\")\n    exit\n  end\n  if(sleep <>nil)then\n    sleep.call({1000})\n  end\nend\n\nstrReport=\"\"\n\ntwinReport = TextWin.make(flnReport, \"DC Water Design\")\n\n' find the SQL Connection\nstrSqlConnection = dicPreferences.get(\"strSqlConnection\")\nif(strSqlConnection = nil)then\n  MsgBox.warning(\"You've got to setup a SQL Connection in order to load Results.\", \"DCWater Design\")\n  return nil\nend\n\nsqlConnection = SqlCon.find(strSqlConnectio
n)\n\nif(sqlConnection.isLogin.not)then\n  av.run(\"DCWatDes.SqlConnection.login\",sqlConnection)\nend\n\nif(strReport.contains(\"Error\").not)then\n  'load results\n  for each strResult in {\"links\", \"nodes\"}\n    flnResult = (flnOut.getFullName+\".\"+strResult+\".txt\").asFilename\n    timeStart=Date.now\n    while(File.exists(flnResult).not)\n      if((Date.now-timeStart).asSeconds>60)then\n        MsgBox.info(\"No Result file found. Check your settings.\",\"DC Water Design Extension\")\n        exit\n      end\n      if(sleep <>nil
